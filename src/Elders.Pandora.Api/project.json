{
  "webroot": "wwwroot",
  "version": "1.0.0-*",

  "dependencies": {
    "Microsoft.AspNet.Mvc": "6.0.0-beta8",
    "Microsoft.AspNet.Server.IIS": "1.0.0-beta7",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-beta8",
    "Microsoft.AspNet.StaticFiles": "1.0.0-beta8",
    "Microsoft.AspNet.WebApi": "5.2.3",
    "log4net": "2.0.3",
    "RestSharp": "105.2.3",
    "Newtonsoft.Json": "6.0.6",
    "LibGit2Sharp": "0.21.0.176",
    "Microsoft.AspNet.Mvc.Core": "6.0.0-beta8",
    "Microsoft.AspNet.Authentication.OAuthBearer": "1.0.0-beta7",
    "System.IdentityModel.Tokens.Jwt": "5.0.0-beta7-208241120",
    "Microsoft.AspNet.Http.Abstractions": "1.0.0-beta8",
    "IdentityServer3.AccessTokenValidation": "2.4.0"
  },

  "commands": {
    "web": "Microsoft.AspNet.Hosting --config hosting.ini"
  },

  "frameworks": {
    "dnx451": {
      "dependencies": {
        "Elders.Pandora": "1.0.0-*",
        "Elders.Pandora.Box": "1.0.0-*",
        "Thinktecture.IdentityModel.Oidc": "1.0.0-*"
      }
    }
  },

  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ],
  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ]
}
